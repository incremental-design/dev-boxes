{
    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "deno",
            "type": "node",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${workspaceFolder}/.vscode/deno",
            "runtimeArgs": [
                "run",
                "--inspect-brk",
                "--allow-all"
            ],
            "attachSimplePort": 9229
        }
    ]
}